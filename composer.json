{
    "name": "brstuff/laravelsqlines",
    "description": "Simple laravel package to manage the sync of databases over Sqlines",
    "license": "MIT",
    "authors": [
        {
            "name": "BRStuff",
            "email": "brstuff3000@gmail.com",
            "homepage": "https://www.brstuff.com"
        }
    ],
    "homepage": "https://github.com/brstuff/laravelsqlines",
    "keywords": ["Laravel", "LaravelSqlines", "sqlines", "brstuff"],
    "require": {
        "illuminate/support": "~5|~6|~7"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "brstuff\\LaravelSqlines\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "brstuff\\LaravelSqlines\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "brstuff\\LaravelSqlines\\LaravelSqlinesServiceProvider"
            ],
            "aliases": {
                "LaravelSqlines": "brstuff\\LaravelSqlines\\Facades\\LaravelSqlines"
            }
        }
    }
}
